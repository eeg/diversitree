## NAMESPACE
useDynLib(diversitreeGSE)

export(make.bisse)
export(make.gse2)
export(make.bd)
export(make.mk2)
export(make.mkn)

## Tree evolution code
export(trees)
export(tree.bisse)
export(tree.bd)

## MLE, MCMC and helpers
export(find.mle)
S3method(find.mle, bisse)
S3method(find.mle, gse2)
S3method(find.mle, bd)

S3method(logLik, mle.bisse)
#S3method(logLik, mle.gse2)
S3method(logLik, mle.bd)
S3method(logLik, mle.mkn)

S3method(anova, mle.bisse)
#S3method(anova, mle.gse2)
S3method(anova, mle.bd)
S3method(anova, mle.mkn)

export(mcmc)
export(constrain)

S3method(print, bisse)
#S3method(print, gse2)
S3method(print, bd)
S3method(print, mkn)
S3method(print, constrained)

export(argnames)
S3method(argnames, bisse)
#S3method(argnames, gse2)
S3method(argnames, bd)
S3method(argnames, mkn)
S3method(argnames, constrained)

export("argnames<-")
S3method("argnames<-", bisse)
#S3method("argnames<-", gse2)
S3method("argnames<-", bd)
S3method("argnames<-", mkn)
S3method("argnames<-", constrained)

export(protect)
export(invert)
export(big.brother)
export(starting.point.bisse)
#export(starting.point.gse2)
export(bisse.starting.point)
export(starting.point)
export(profiles.plot)

## Support for "clade.tree"s
export(polytomies.to.clades)
export(make.clade.tree)
S3method(plot, clade.tree)
